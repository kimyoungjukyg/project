<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtdmybatis-3-mapper.dtd">
<mapper namespace="Card">
<insert parameterType="card" id="cardlist"> INSERT INTO cardlist VALUES(#{id},#{cardnum},#{password}) </insert>
<select id="cardread" parameterType="card" resultType="card">
SELECT * FROM cardlist 
</select>

</mapper>